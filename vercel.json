{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/movies",
      "headers": {
        "methods": [
          "GET"
        ],
        "Location": "https://admirable-caramel-1c2dbd.netlify.app/"
      }
    },
    {
      "src": "/movies/:id",
      "headers": {
        "methods": [
          "GET"
        ],
        "Location": "https://admirable-caramel-1c2dbd.netlify.app/"
      }
    },
    {
      "src": "/movies/movie/:id",
      "methods": [
        "GET"
      ],
      "headers": {
        "Location": "https://admirable-caramel-1c2dbd.netlify.app/"
      }
    }
  ]
}